//https://leetcode.com/problems/rectangle-overlap/

class Solution {
public:
    bool isRectangleOverlap(vector<int>& r1, vector<int>& r2) {
        
        
        int x1 ( r1 [ 0 ] ) , x2 ( r1 [ 2 ] )  , x3 ( r2 [ 0 ] ) , x4 ( r2 [ 2] ) , y1 ( r1 [ 1 ] ), y2 ( r1 [ 3 ] ) , y3 ( r2 [ 1 ]), y4( r2 [ 3 ] )  ;
       if ( y3 >= y2 or y1 >= y4 or x3>= x2 or x4 <= x1 )
            return 0 ; 
        
        return 1 ; 
        
        
    }
};
